#=========================================================================
# Convert LIB files to DB
#=========================================================================

#-------------------------------------------------------------------------
# Get the parameters
#-------------------------------------------------------------------------

set lib_name                $::env(lib_name)
set lib_filename            $::env(lib_filename)
set db_filename             $::env(db_filename)

#-------------------------------------------------------------------------
# commands
#-------------------------------------------------------------------------

read_lib $lib_filename
write_lib $lib_name -format db -output $db_filename

#-------------------------------------------------------------------------
# done
#-------------------------------------------------------------------------

exit
